  "name": "Proxifly Proxy Switcher",
  "version": "0.1.0",
  "description": "Switch proxy browser dengan daftar dari proxifly/free-proxy-list.",
  "permissions": [
    "proxy",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://cdn.jsdelivr.net/*",
    "https://vwmhbpgwhfwuwtattset.supabase.co/*",
    "https://ip8.com/*",
    "https://api.ipify.org/*",
    "https://api64.ipify.org/*"
  ],
